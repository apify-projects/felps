{
    "extends": "../../../tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "target": "esnext",
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2015",
            "ES2016",
            "ES2018",
            "ES2019.Object",
            "ES2018.AsyncIterable",
            "ES2020.String",
            "ES2019.Array"
        ],
        "rootDir": "src",
        "outDir": "lib"
    },
    "exclude": [
        "node_modules",
        "lib"
    ],
    "include": [
        "src"
    ],
    "references": []
}