{
    "extends": "../tsconfig.json",
    "include": ["**/*", "../packages/**/src"],
    "compilerOptions": {
        "sourceMap": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strictNullChecks": false,
        "paths": {
            "felps": ["../packages/core/felps"],
            "@usefelps/*": [
                "../packages/adapters/*",
                "../packages/apify/*",
                "../packages/core/*",
                "../packages/crawlee/*",
                "../packages/helpers/*"
            ]
        }
    }
}
